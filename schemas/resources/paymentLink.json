{
    "parameters": [
        {
            "name": "operation",
            "meta": {
                "displayName": "Operation",
                "displayType": "select",
                "placeholder": "Select an operation",
                "description": "Select the operation you want to perform.",
                "options": [
                    {
                        "label": "Create Payment Link",
                        "value": "paymentLink.createPaymentLink",
                        "description": "Create a payment link using basic details such as amount and expiry date."
                    },
                    {
                        "label": "Get All Payment Links",
                        "value": "paymentLink.getPaymentLinks",
                        "description": "Get details of all standard payment links."
                    },
                    {
                        "label": "Get Payment Link Details by ID",
                        "value": "paymentLink.getPaymentLinkById",
                        "description": "Get payment link details using the payment link ID."
                    }
                ],
                "validation": {
                    "required": true
                },
                "dependencies": {
                    "conditions": [
                        {
                            "field": "resource",
                            "operator": "EQUALS",
                            "value": "paymentLink"
                        }
                    ]
                }
            }
        }
    ],
    "createPaymentLink": {
        "definition": {
            "method": "POST",
            "url": "https://api.razorpay.com/v1/payment_links",
            "headers": {
                "Content-Type": "application/json",
                "Authorization": "Basic convertBufferBase64String({{secrets.client_id}}:{{secrets.client_secret}})"
            },
            "body": {
                "amount": "{{parameters.amount}}",
                "currency": "{{parameters.currency}}",
                "reference_id": "{{parameters.reference_id}}",
                "description": "{{parameters.description}}",
                "notes": "{{parameters.notes}}",
                "callback_url": "{{parameters.callback_url}}",
                "callback_method": "{{parameters.callback_url ? parameters.callback_method : null}}",
                "customer": "{{parameters.customer}}",
                "reminder_enable": "{{parameters.reminder_enable}}",
                "notify": {
                    "sms": "{{parameters.notify_sms}}",
                    "email": "{{parameters.notify_email}}"
                },
                "expire_by": "{{parameters.expire_by}}",
                "upi_link": "{{parameters.upi_link}}",
                "accept_partial": "{{parameters.accept_partial}}",
                "first_min_partial_amount": "{{parameters.accept_partial ? parameters.first_min_partial_amount : null}}"
            },
            "transform": {
                "body": "const obj = JSON.parse(JSON.stringify(body)); if (obj.customer && typeof obj.customer.contact === 'string') { const cleaned = obj.customer.contact.replace(/\\s+/g, ''); obj.customer.contact = cleaned === '' ? null : cleaned; } return obj;"
            },
            "response": {
                "output": "{{response.data}}",
                "error": {
                    "message": "{{response.data.error.description}}",
                    "code": "{{response.status}}"
                }
            }
        },
