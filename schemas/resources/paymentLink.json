                "name": "amount",
                "meta": {
                    "displayName": "Amount",
                    "displayType": "number",
                    "placeholder": "Enter the amount",
                    "description": "Amount in the smallest unit of the currency. For example, to receive 300, enter 30000.",
                    "validation": {
                        "required": true
                    },
                    "htmlProps": {
                        "allowDynamic": true
                    }
                }
            },
            {
                "name": "accept_partial",
                "meta": {
                    "displayName": "Accept Partial Payments",
                    "description": "Allow customers to make partial payments using this payment link.",
                    "displayType": "toggle",
                    "placeholder": "Accept partial payments",
                    "readOnly": false,
                    "value": false,
                    "validation": {
                        "disabled": false
                    },
                    "htmlProps": {
                        "allowDynamic": true
                    }
                }
            },
            {
                "name": "first_min_partial_amount",
                "meta": {
                    "displayName": "Minimum First Partial Amount",
                    "displayType": "number",
                    "placeholder": "Enter the minimum first partial amount",
                    "description": "Minimum amount for the first partial payment (in the smallest unit of the currency).",
                    "validation": {
                        "required": true
                    },
                    "htmlProps": {
                        "allowDynamic": true
                    },
                    "dependencies": {
                        "conditions": [
                            {
                                "field": "accept_partial",
                                "value": true,
                                "operator": "EQUALS"
                            }
                        ]
                    }
                }
            },
            {
                "name": "reference_id",
                "meta": {
                    "displayName": "Reference ID",
                    "displayType": "text",
                    "placeholder": "Enter a reference ID",
