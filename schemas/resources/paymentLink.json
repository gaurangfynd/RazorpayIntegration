                    "displayName": "Description",
                    "displayType": "textarea",
                    "placeholder": "Enter a payment link description",
                    "description": "Description shown to the customer on the payment link.",
                    "validation": {
                        "required": false,
                        "maxLength": 2048
                    },
                    "htmlProps": {
                        "minRows": 4,
                        "maxRows": 4
                    }
                }
